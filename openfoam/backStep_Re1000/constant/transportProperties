// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Re = U * H / nu = 1000
// With U = 1 and H = 1, nu = 1/1000 = 0.001

transportModel  Newtonian;

nu              0.001;  // Kinematic viscosity for Re=1000 with U=1 and H=1

